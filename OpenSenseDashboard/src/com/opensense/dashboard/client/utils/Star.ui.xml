<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" 
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:m="urn:import:gwt.material.design.client.ui">
	
	<ui:with type="com.opensense.dashboard.client.utils.Languages" field="lang" />
	<ui:with type="com.opensense.dashboard.client.gui.GUIImageBundle" field="gui" />
	
<ui:style>
.star-five {
  transform: scale(0.15) rotate(35deg);
  color: rgba(255, 220, 0);
  width: 0px;
  height: 0px;
  border-right: 100px solid transparent;
  border-bottom: 70px solid rgba(255, 220, 0);
  border-left: 100px solid transparent;
  z-index: 3;
}
.star-five:before {
  border-bottom: 80px solid rgba(255, 220, 0);
  border-left: 30px solid transparent;
  border-right: 30px solid transparent;
  position: absolute;
  height: 0;
  width: 0;
  top: -45px;
  left: -65px;
  display: block;
  content: '';
  transform: rotate(-35deg);
}
.star-five:after {
  position: absolute;
  display: block;
  color: rgba(255, 220, 0);
  top: 3px;
  left: -105px;
  width: 0px;
  height: 0px;
  border-right: 100px solid transparent;
  border-bottom: 70px solid rgba(255, 220, 0);
  border-left: 100px solid transparent;
  transform: rotate(-70deg);
  content: '';
}
</ui:style>
   
<bh:Div addStyleNames="{style.star-five}"/>
   
</ui:UiBinder>